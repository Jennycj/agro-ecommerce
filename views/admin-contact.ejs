	<title>Greenfarm - Organic Food eCommerce Bootstrap 4 Template</title>
	<meta name="description" content="">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<!-- Favicon -->
  <link rel="icon" href="./images/favicon.ico">
  <!-- <link rel="stylesheet" href="index1.ejs"> -->

	<!-- CSS
	============================================ -->
	<!-- Bootstrap CSS -->
	<link href="./css/bootstrap.min.css" rel="stylesheet">

	<!-- FontAwesome CSS -->
	<link href="./css/font-awesome.min.css" rel="stylesheet">

	<!-- Elegent CSS -->
	<link href="./css/elegent.min.css" rel="stylesheet">

	<!-- Plugins CSS -->
	<link href="./css/plugins.css" rel="stylesheet">

	<!-- Helper CSS -->
	<link href="./css/helper.css" rel="stylesheet">

	<!-- Main CSS -->
	<link href="./css/main.css" rel="stylesheet">

	<!-- Modernizer JS -->
	<script src="./js/vendor/modernizr-2.8.3.min.js"></script>

</head>

<body>

	<!--=============================================
	=            Header         =
	=============================================-->


	<header>
		<!--=======  header top  =======-->

		<div class="header-top pt-10 pb-10 pt-lg-10 pb-lg-10 pt-md-10 pb-md-10">
			<!-- <div class="container">

					</div>  -->
				</div>
			</div>
		</div>

		<!--=======  End of header top  =======-->

		<!--=======  header bottom  =======-->

		<div class="header-bottom header-bottom-one header-sticky">
			<div class="container">
				<div class="row">
					<div class="col-md-3 col-sm-12 col-xs-12 text-lg-left text-md-center text-sm-center">
						<!-- logo -->
						<div class="logo mt-15 mb-15">
							<a href="index1.ejs">
								<img src="images/AGRO2.png" class="img-fluid" alt="">
							</a>
						</div>
						<!-- end of logo -->
					</div>
					<div class="col-md-9 col-sm-12 col-xs-12">
						<div class="menubar-top d-flex justify-content-between align-items-center flex-sm-wrap flex-md-wrap flex-lg-nowrap mt-sm-15">
							<!-- header phone number -->
							<div class="header-contact d-flex">
								<div class="phone-icon">
									<img src="./images/icon-phone.png" class="img-fluid" alt="">
								</div>
								<div class="phone-number">
									 <span class="number"> Phone: +234-813-6300-744  |  +234-816-5051-594  |  +234-810-3612-164</span>
								</div>
							</div>

						</div>

						<!-- navigation section -->
                        <div class="main-menu">
							<nav>
								<ul>
									<li class="active menu-item"><a href="/dashboard">DASHBOARD</a>
										<!-- <ul class="sub-menu"> -->
											<li><a href="/"></a></li>

										<!-- </ul> -->
									</li>
									<li class="menu-item"><a href="/productadd">ADD PRODUCT</a></li>


													<li><a href="/adminProd">PRODUCT</a></li>
                          <li class="menu-item"><a href="/admin-contact">CONTACT MESSAGES</a></li>
								</ul>
							</nav>
						</div>
						<!-- end of navigation section -->
					</div>
					<div class="col-12">
						<!-- Mobile Menu -->
						<div class="mobile-menu d-block d-lg-none"></div>
					</div>
				</div>
			</div>
		</div>

		<!--=======  End of header bottom  =======-->
	</header>
	<!--=====  End of Header  ======-->

    <!--=============================================
    =            breadcrumb area         =
    =============================================-->

    <div class="breadcrumb-area mb-50">
        <div class="container">
            <div class="row">
                <div class="col">
                    <div class="breadcrumb-container">
                        <ul>
                            <li><a href="index1.ejs"><i class="fa fa-home"></i> Home</a></li>
                            <li class="active">Contact</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>

	<!--=====  End of breadcrumb area  ======-->


	<!--=============================================
	=            Contact page content         =
	=============================================-->
<div class="page-section section mb-50">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <form action="">
                        <!--=======  cart table  =======-->
                        <div class="cart-table table-responsive mb-40">
                          <table class="table">
                            <thead>
                              <tr>
                                <th class="pro-title">Full Name</th>
                                <th class="pro-price">Email</th>
                                <th class="pro-quantity">Subject</th>
                                <th class="pro-subtotal">Description</th>

                              </tr>
                            </thead>
                            <tbody>
                                <% contacts.forEach(item => { %>
                                      <tr>
                                        <td class="pro-title"><%= item.fullName %></td>
                                        <td class="pro-price"><%= item.email %></td>
                                        <td class="pro-quantity"><%= item.subject %></td>
                                        <td class="pro-subtotal"><%= item.message %></td>
                                      </tr>
                                    <% } )%>

                                </tbody>
                            </table>
                        </div>

                        <!--=======  End of cart table  =======-->


                      </form>

	<!--=====  End of Contact page content  ======-->


	<!--=============================================
	=            Footer         =
	=============================================-->

	<footer class="sticky-footer bg-white">
    <div class="container my-auto">
        <div class="copyright text-center my-auto">
            <span>Copyright &copy; Your Website 2021</span>
        </div>
    </div>
</footer>

	<!--=====  End of Footer  ======-->


	<!-- scroll to top  -->
	<a href="#" class="scroll-top"></a>
	<!-- end of scroll to top -->

	<!-- JS
	============================================ -->
