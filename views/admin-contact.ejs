	<!-- Bootstrap JS -->
	<script src="./js/bootstrap.min.js"></script>

	<!-- Plugins JS -->
	<script src="./js/plugins.js"></script>

	<!-- Main JS -->
	<script src="./js/main.js"></script>

	<!-- AJAX mail JS -->
	<script src="./js/ajax-mail.js"></script>


</body>

</html>
